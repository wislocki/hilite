  [/^INFO/, BLUE],
  [/^WARN(ING)?/, YELLOW],
  [/^(ERROR|FATAL)/, RED],
  [/^DEBUG/, GREEN],
  [/^[A-Z][a-z][a-z] .+ [A-Z][A-Z][A-Z] 20[0-9][0-9] .*$/, WHITE, DIM],
  [/[A-Z][A-Za-z0-9_$]+\.(java|scala)/, MAGENTA],
  [/at (sbt|scala|org|sun|java(x)?|com)\.[A-Za-z0-9_.$]+/, BLUE],
  [/at org\.wislocki\.[A-Za-z0-9_.$]+/, CYAN],
  [/[A-Za-z0-9_$.]+(Exception|Error)$/, RED],
  [/^[A-Za-z0-9_$.]+(Exception|Error)/, RED]
]