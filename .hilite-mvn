$hilites = [
  [/INFO/, BLUE],
  [/WARN(ING)?/, YELLOW],
  [/(ERROR|FATAL)/, RED],
  [/Download(ing|ed)/, BLUE],
  [/Upload(ing|ed)/, MAGENTA],
  [/(BUILD )FAILURE/, RED],
  [/BUILD SUCCESS/, GREEN],
  [/-----+/, BLUE],
  [/--- .+ ---/, YELLOW],
  [/[A-Z][a-z][a-z] .+(AM|PM) .*$/, WHITE, DIM],
  [/[A-Z][A-Za-z0-9_$]+\.java/, MAGENTA],
  [/at (scala|org|sun|java(x)?|com)\.[A-Za-z0-9_.$]+/, BLUE],
  [/at org\.wislocki\.[A-Za-z0-9_.$]+/, CYAN],
  [/[A-Za-z0-9_$.]+(Exception|Error)$/, RED],
  [/^[A-Za-z0-9_$.]+(Exception|Error)/, RED],
  [/^Running .+$/, CYAN],
  [/ Errors: [1-9][0-9]*/, RED],
  [/ Errors: 0/, GREEN],
  [/ Failures: [1-9][0-9]*/, RED],
  [/ Failures: 0/, GREEN],
  [/ Skipped: [1-9][0-9]*/, YELLOW],
  [/ Skipped: 0/, GREEN]
]
